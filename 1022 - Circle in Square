#include<bits/stdc++.h>
#include<iostream>
#include<cstdio>
#include<cmath>

/*
Nayeem Mollick Joy , Electrical & Electronic Engineering , University of Rajshahi.
Passionate Contest Programmer , Coder , Android Developer */
 
#define pi 2*acos(0.0)
using namespace std;
 
int main()
 
{
    double T,r,s=0,ans=0,l;
 
    cin>>T;
 
    for(int i=1;i<=T;i++)
    {
        cin>>r;
 
        s=2*r*2*r;
 
        ans=s-(pi*r*r);
 
        cout<<"Case "<<i<<": ";
 
        printf("%.02lf\n",ans);
    }
 
    return 0;
}
