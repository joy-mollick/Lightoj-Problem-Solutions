#include<bits/stdc++.h>
#include<iostream>
#include<cstdio>
#include<cmath>
 
 
#define pf printf
#define sf scanf
using namespace std;
 
    int main()
    {
        int T,N;
        sf("%d\n",&T);
 
        int i,j,a;
        unsigned long long int sum = 0;
        for(i =0; i<T;i++)
        {
            sf("%d",&N);
            for(j=0;j<N;j++)
            {
                scanf("\n%d",&a);
                if(a>0)
                sum+=a;
 
 
            }
            pf("Case %d: %lld\n",i+1,sum);
            sum =0;
 
        }
       return 0;
    }
